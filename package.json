{
  "name": "react-observatory",
  "version": "1.1.0",
  "description": "Dynamic reducer and epic injection for Redux Observable apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/halfzebra/react-observatory"
  },
  "bugs": {
    "url": "https://github.com/halfzebra/react-observatory/issues"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "functional",
    "observable",
    "stream",
    "rxjs"
  ],
  "author": "Eduard Kyvenko <eduard.kyvenko@gmail.com> (https://github.com/halfzebra)",
  "license": "MIT",
  "module": "es/react-observatory.js",
  "main": "lib/react-observatory.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "lint": "eslint src test build",
    "pretest": "npm run build:commonjs",
    "test": "cross-env NODE_ENV=testing BABEL_ENV=testing jest",
    "test:watch": "cross-env NODE_ENV=testing npm test -- --watch",
    "test:cov": "cross-env NODE_ENV=testing npm test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/react-observatory.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/react-observatory.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/react-observatory.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/react-observatory.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test && npm run build",
    "examples:lint": "eslint examples",
    "examples:test": "cross-env CI=true babel-node examples/testAll.js",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "redux": "^3.7.2",
    "react": "^16.2.0"
  },
  "optionalDependencies": {
    "redux-observable": "^0.18.0",
    "react-router": "^4.2.0",
    "rxjs": "^5.5.6"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "^2.2.3",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rxjs": "^5.5.6"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "testRegex": "(.*\\.spec.js)$",
    "setupTestFrameworkScriptFile": "<rootDir>/src/__tests__/testSetup.js",
    "moduleFileExtensions": [
      "js"
    ]
  }
}
